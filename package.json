{
	"name": "typora-uploader",
	"version": "1.0.2",
	"description": "out-of-the-box typora image uploader",
	"bin": {
		"typora-uploader": "./bin/index"
	},
	"files": [
		"assets",
		"bin",
		"lib"
	],
	"keywords": [
		"typora",
		"image",
		"upload",
		"qq"
	],
	"author": {
		"name": "cong99"
	},
	"scripts": {
		"changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"commitizen": "^4.1.2",
		"conventional-changelog-cli": "^2.0.34",
		"cz-conventional-changelog": "^3.2.0",
		"husky": "^4.2.5"
	},
	"dependencies": {
		"chalk": "^4.1.1",
		"form-data": "^4.0.0",
		"minimist": "^1.2.5",
		"request": "^2.88.2",
		"sharp": "^0.29.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cong99/typora-uploader.git"
	},
	"bugs": {
		"url": "https://github.com/cong99/typora-uploader/issues"
	},
	"homepage": "https://github.com/cong99/typora-uploader",
	"license": "ISC"
}
